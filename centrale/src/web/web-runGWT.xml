<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <servlet>
    <servlet-name>centraleUI</servlet-name>
    <servlet-class>it.csi.centrale.ui.server.CentraleUIServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>centraleUI</servlet-name>
    <url-pattern>/it.csi.centrale.ui.CentraleUI/uiservice</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>mapsService</servlet-name>
    <servlet-class>it.csi.centrale.ui.server.MapsService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mapsService</servlet-name>
    <url-pattern>/it.csi.centrale.ui.CentraleUI/mapsService</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>displayChart</servlet-name>
    <servlet-class>org.jfree.chart.servlet.DisplayChart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>displayChart</servlet-name>
    <url-pattern>/it.csi.centrale.ui.CentraleUI/displayChart</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>exportService</servlet-name>
    <servlet-class>it.csi.centrale.servlet.ExportService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>exportService</servlet-name>
    <url-pattern>/it.csi.centrale.ui.CentraleUI/exportService</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>perifService</servlet-name>
    <servlet-class>it.csi.centrale.servlet.PerifService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>perifService</servlet-name>
    <url-pattern>/it.csi.centrale.ui.CentraleUI/perifService</url-pattern>
  </servlet-mapping>
  
  <session-config>
  	<session-timeout>1</session-timeout>
  </session-config>
</web-app>
